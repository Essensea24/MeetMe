<h1>Hello <%=@user.firstname%></h1>
<p>Firstname:<%=@user.firstname%></p><br>
<p>Lastname:<%=@user.lastname%></p><br>
<p>Username:<%=@user.username%></p><br>
<p>Email:<%=@user.email%></p><br>
<p><%= link_to "Users Page", users_path%></p><br>
<p><%= link_to "edit", edit_user_path(@user)%></p><br>
<p><%= link_to "Delete", user_path(@user), method: :delete, data: { confirm: "Are you sure you want to delete your account?"} %></p>